<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".houseq4">

    <TextView
        android:id="@+id/textView"
        android:layout_width="342dp"
        android:layout_height="82dp"
        android:layout_marginTop="254dp"
        android:layout_marginBottom="392dp"
        android:text="What type of energy do you use to heat your home?
"
        android:textSize="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/ptq12"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#009999"
        android:text="Electricity"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/atq1no" />

    <Button
        android:id="@+id/ptq15"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#009999"
        android:text="Oil"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.296"
        app:layout_constraintStart_toEndOf="@+id/ptq12"
        app:layout_constraintTop_toBottomOf="@+id/atq1no" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="256dp"
        android:layout_height="205dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ptq110"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginBottom="56dp"
        android:backgroundTint="#009999"
        android:text="Propane"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ptq1more"
        app:layout_constraintHorizontal_bias="0.674"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ptq12"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/ptq1more"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#009999"
        android:text="Wood"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ptq15"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/atq1no"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#009999"
        android:text="Natural Gas"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/carq2more"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginBottom="56dp"
        android:backgroundTint="#009999"
        android:text="Other"

        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/atq1no"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>