<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".carq1">

    <TextView
        android:id="@+id/textView"
        android:layout_width="342dp"
        android:layout_height="82dp"
        android:layout_marginTop="254dp"
        android:layout_marginBottom="392dp"
        android:text="What type of car do you drive?
"
        android:textSize="34dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/carq1g"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#009999"
        android:text="Gasoline"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carq1no" />

    <Button
        android:id="@+id/carq1d"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#009999"
        android:text="Diesel"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.296"
        app:layout_constraintStart_toEndOf="@+id/carq1g"
        app:layout_constraintTop_toBottomOf="@+id/carq1no" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="256dp"
        android:layout_height="205dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/carq1h"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginBottom="56dp"
        android:backgroundTint="#009999"
        android:text="Hybrid"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/carq1e"
        app:layout_constraintHorizontal_bias="0.674"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carq1g"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/carq1e"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#009999"
        android:text="Electric"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/carq1d"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/carq1no"
        android:layout_width="132dp"
        android:layout_height="82dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#009999"
        android:text="I dont't know"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>